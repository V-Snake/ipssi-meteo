        "content": "<div style='text-align: center; padding: 20px; background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); border-radius: 15px; color: white; box-shadow: 0 4px 15px rgba(0,0,0,0.2);'><h3>üå§Ô∏è M√©t√©o en Temps R√©el</h3><div id='weather-info' style='margin-top: 15px; padding: 15px; background: rgba(255,255,255,0.1); border-radius: 10px;'><p><strong>Spot s√©lectionn√©:</strong> <span id='selected-spot-name'>Aucun</span></p><p><strong>Derni√®re mise √† jour:</strong> <span id='last-update'>-</span></p></div><div style='margin-top: 15px;'><button onclick='refreshWeather()' style='background: #4CAF50; color: white; border: none; padding: 10px 20px; border-radius: 5px; cursor: pointer; margin: 5px;'>üîÑ Actualiser</button><button onclick='selectRandomSpot()' style='background: #2196F3; color: white; border: none; padding: 10px 20px; border-radius: 5px; cursor: pointer; margin: 5px;'>üé≤ Spot Al√©atoire</button></div></div><script>let selectedSpotId = '${spot_id}'; function refreshWeather() { fetchWeatherData(selectedSpotId); } function selectRandomSpot() { const spots = [670699720, 913120839, 123456789]; selectedSpotId = spots[Math.floor(Math.random() * spots.length)]; fetchWeatherData(selectedSpotId); } function fetchWeatherData(spotId) { fetch(`/weather/${spotId}`).then(response => response.json()).then(data => { if (data.status === 'ok') { document.getElementById('selected-spot-name').textContent = data.data.spot.name; document.getElementById('last-update').textContent = new Date().toLocaleTimeString(); updateWeatherPanels(data.data); } }); } function updateWeatherPanels(weatherData) { console.log('Weather data updated:', weatherData); } setInterval(refreshWeather, 30000); refreshWeather();</script>",
